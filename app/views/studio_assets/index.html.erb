<h1>Listing studio_assets</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Base cost</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @studio_assets.each do |studio_asset| %>
  <tr>
    <td><%= studio_asset.name %></td>
    <td><%= studio_asset.base_cost %></td>
    <td><%= link_to 'Show', studio_asset %></td>
    <td><%= link_to 'Edit', edit_studio_asset_path(studio_asset) %></td>
    <td><%= link_to 'Destroy', studio_asset, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

